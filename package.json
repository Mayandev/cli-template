{
  "name": "node-cli-template",
  "version": "0.0.1",
  "description": "A template repository for node command line interface",
  "main": "index.js",
  "bin": {
    "template": "bin/main.js"
  },
  "scripts": {
    "format": "prettier --write '**/*.{json,js,md,yml}'",
    "pretty-quick": "pretty-quick",
    "prepublishOnly": "np --no-cleanup --no-publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mayandev/flomo-cli"
  },
  "keywords": [
    "template",
    "cli"
  ],
  "author": "phillzou <phillzou@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mayandev/flomo-cli/issues"
  },
  "homepage": "https://github.com/Mayandev/flomo-cli#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "command-exists": "^1.2.9",
    "commander": "^6.2.1",
    "execa": "^5.0.0",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "eslint": "^7.17.0",
    "husky": "^4.3.7",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "np": "^7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
